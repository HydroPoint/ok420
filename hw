    delay(pausa);
  }

  edoETAPACLN = SD.open("edoETAPACLN.txt", FILE_READ);
  if (edoETAPACLN)
  {
    file1SIZE10 = edoETAPACLN.size() * 0.000001;
    edoETAPACLN.close();
    //Serial.print("File size: ");
    //Serial.print(file1SIZE10);
    //Serial.println(" (Mb)");
  }
  else
